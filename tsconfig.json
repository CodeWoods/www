{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": ".",
    // "paths": { "#/*": ["./src/*"] },
    "paths": { "~/*": ["./src/*"] },
    "strictNullChecks": true,
    "allowJs": true,
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "types": ["@astrojs/client"]
  },
  "exclude": ["node_modules"],
  "include": ["**/*", ".eslintrc.cjs"]
}
