{
  "name": "@codewoods/www",
  "type": "module",
  "version": "0.0.0-development",
  "private": true,
  "license": "MIT",
  "homepage": "https://github.com/codewoods/www",
  "scripts": {
    "dev": "export NODE_ENV=development && astro dev",
    "start": "npm run dev",
    "build": "export NODE_ENV=production && astro build",
    "build:p": "npm run build && npm run preview",
    "preview": "astro preview",
    "astro": "astro",
    "typecheck": "astro check && tsc --noEmit",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "fmt": "prettier --write \"**/*.{js,cjs,mjs,jsx,ts,tsx,json,css,scss,astro}\"",
    "lint": "eslint --ext .js,.ts,.tsx,.astro ./src --ignore-path .gitignore",
    "clear": "node scripts/clear.js",
    "clean": "node scripts/clean.js",
    "deploy": "firebase deploy --only hosting",
    "deploy:mock": "firebase emulators:start",
    "deploy:stage": "firebase hosting:channel:deploy stage --expires 10m"
  },
  "dependencies": {
    "@astrojs/prefetch": "^0.1.2",
    "@astrojs/prism": "^1.0.2",
    "@astrojs/rss": "^2.0.0",
    "@astrojs/sitemap": "^1.0.0",
    "@astrojs/tailwind": "^2.1.3",
    "@markdoc/markdoc": "^0.2.1",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.8",
    "astro": "^1.8.0",
    "astro-compress": "1.1.32",
    "astro-markdoc-renderer": "0.0.1-alpha.1",
    "astro-remark-eleventy-image": "^1.2.1",
    "date-fns": "^2.29.3",
    "globby": "^13.1.3",
    "gray-matter": "^4.0.3",
    "remark-pangu": "^2.2.0",
    "slugify": "^1.6.5",
    "tailwindcss": "^3.2.4",
    "unist-util-visit": "^4.1.2",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@secretlint/secretlint-rule-preset-recommend": "^6.2.3",
    "@types/eslint": "^8.21.0",
    "@types/prettier": "^2.7.2",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "@vitest/coverage-c8": "^0.28.4",
    "@vitest/ui": "^0.28.4",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-astro": "^0.23.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "happy-dom": "^8.4.0",
    "lefthook": "^1.2.9",
    "prettier": "^2.8.4",
    "prettier-plugin-astro": "0.8.0",
    "prettier-plugin-tailwindcss": "^0.2.2",
    "secretlint": "^6.2.3",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vitest": "^0.28.4"
  },
  "browserslist": [
    "cover 80% in alt-AS",
    "maintained node versions",
    "Firefox ESR",
    "not dead",
    "iOS >= 13"
  ]
}
